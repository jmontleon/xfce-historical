XFCE_VERSION=3.8.14
xfce_version=`grep version xfce.spec | grep define | sed -e 's,[[^0-9.]],,g'`
case `echo $xfce_version | sed -e "s,$XFCE_VERSION,DEVELOPMENTVERSION,"` in
  DEVELOPMENTVERSION.*) 
     AC_MSG_RESULT(:changed $XFCE_VERSION into $xfce_version as in xfce.spec) 
     XFCE_VERSION=$xfce_version ;;
  *) AC_MSG_RESULT(... seen $xfce_version in xfce.spec but did not do anything)     ;;
esac

