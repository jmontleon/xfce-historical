PACKAGE = @PACKAGE@
VERSION = @VERSION@
XFCE_DIR = @XFCE_DIR@

SHELL = /bin/sh
@SET_MAKE@

srcdir = @srcdir@
top_srcdir = @top_srcdir@
VPATH = @srcdir@

prefix = @prefix@
exec_prefix = @exec_prefix@
xfcedir = $(XFCE_DIR)
subdir = other

INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@
MKINSTALLDIRS = $(top_srcdir)/@MKINSTALLDIRS@

CC = @CC@
GENCAT = @GENCAT@
GMSGFMT = PATH=../src:$$PATH @GMSGFMT@
MSGFMT = @MSGFMT@
XGETTEXT = PATH=../src:$$PATH @XGETTEXT@
MSGMERGE = PATH=../src:$$PATH msgmerge

DEFS = @DEFS@
CFLAGS = @CFLAGS@
CPPFLAGS = @CPPFLAGS@

DISTFILES = \

CATALOGS = @CATALOGS@
CATOBJEXT = @CATOBJEXT@
INSTOBJEXT = @INSTOBJEXT@

.SUFFIXES:

all: COPYING

COPYING:
	sed -e "s%XFCE_VERSION%$(VERSION)%g" \
	-e "s%DATE%`date`%g" \
	-e "s%HOSTNAME%`hostname`%g" COPYING.in > COPYING
	
install: all install-exec install-data
install-exec:
install-data:
	if test -r "$(MKINSTALLDIRS)"; then \
	  $(MKINSTALLDIRS) $(DESTDIR)/$(xfcedir); \
	else \
	  $(SHELL) $(top_srcdir)/mkinstalldirs $(DESTDIR)/$(xfcedir); \
	fi
	@catalogs='$(DISTFILES)'; \
	for cat in $$catalogs; do \
	  $(INSTALL_DATA) $$cat $(DESTDIR)/$(xfcedir); \
	done

# Define this as empty until I found a useful application.
installcheck:

uninstall:
	@catalogs='$(DISTFILES)'; \
	for cat in $$catalogs; do \
	  rm -f $(xfcedir)/$$cat; \
	done

check: all

dvi info tags TAGS ID:

mostlyclean:
	rm -f core core.*
	rm -fr *.o

clean: mostlyclean

distclean: clean
	rm -f Makefile Makefile.in sample.xfbdrc sample.xfce3rc sample.xfsoundrc \
	      sample.xfwmrc system.xfwmrc COPYING sample.xfce3rc.ja sample.xfce3rc.ko \
	      sample.xfce3rc.zh_CN.GB2312 sample.xfce3rc.zh_TW.Big5

maintainer-clean: distclean

distdir = ../$(PACKAGE)-$(VERSION)/$(subdir)
dist distdir: 

Makefile: Makefile.in.in ../config.status
	cd .. \
	  && CONFIG_FILES=$(subdir)/$@.in CONFIG_HEADERS= \
	       $(SHELL) ./config.status

# Tell versions [3.59,3.63) of GNU make not to export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
